{
  "version": 2,
  "builds": [
    {
      "src": "backend/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/food/(.*)",
      "dest": "/backend/routes/foodRoutes.js"
    },
    {
      "src": "/api/cart/(.*)",
      "dest": "/backend/routes/cartRoutes.js"
    },
    {
      "src": "/api/order/(.*)",
      "dest": "/backend/routes/orderRoutes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/server.js"
    }
  ]
}
